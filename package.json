{
  "name": "webpack-starter-kit",
  "version": "0.1.0",
  "description": "Base template for webpack application.",
  "main": "./app/scripts/app.js",
  "devPort": 8008,
  "scripts": {
    "deploy": "NODE_ENV=production webpack --config webpack.config.js",
    "server": "NODE_ENV=development PORT=$npm_package_devPort webpack-dev-server --config webpack.config.js",
    "start": "npm run server"
  },
